{
  "name": "kimono-coordinator",
  "private": true,
  "type": "module",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "check": "run-p --aggregate-output check:{lint,format,type}",
    "check:lint": "oxlint",
    "check:lint:withtype": "oxlint --type-aware --type-check",
    "check:format": "oxfmt --check",
    "check:type": "tsgo -b",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "fix:lint": "oxlint --fix",
    "fix:format": "oxfmt --write"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router": "^1.151.6",
    "@tanstack/react-router-devtools": "^1.151.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^2.1.12",
    "@openai/codex": "^0.87.0",
    "@tanstack/router-plugin": "^1.151.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260115.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "jsdom": "^27.4.0",
    "lefthook": "^2.0.13",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "oxlint-tsgolint": "^0.11.0",
    "shadcn": "^3.6.3",
    "tw-animate-css": "^1.4.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "engines": {
    "npm": "please-use-pnpm"
  },
  "packageManager": "pnpm@10.28.1"
}
